<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<GridPane fx:id="panelTileSetting" accessibleRole="DECREMENT_BUTTON" alignment="CENTER" hgap="10.0" maxHeight="400.0"
          maxWidth="830.0" minHeight="400.0" minWidth="830.0" prefHeight="400.0" prefWidth="830.0" vgap="10.0"
          xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="com.walkgis.tiles.web.ctrl.PanelTileSettingController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="165.0" minHeight="10.0" prefHeight="52.0" valignment="CENTER" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="200.0" minHeight="10.0" prefHeight="200.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="182.0" minHeight="10.0" prefHeight="125.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <VBox fx:id="containerOutputDir" alignment="CENTER" GridPane.columnIndex="0" GridPane.rowIndex="1">
            <children>
                <AnchorPane fx:id="panelOutputDir" minHeight="-Infinity" prefHeight="-1.0" prefWidth="-1.0"
                            VBox.vgrow="NEVER">
                    <children>
                        <ImageView cache="false" fitHeight="58.0" fitWidth="86.0" focusTraversable="true" layoutX="0.0"
                                   layoutY="54.0" pickOnBounds="true" preserveRatio="false" rotate="0.0" smooth="false"
                                   visible="true">
                            <image>
                                <Image url="@../../../img/outputDir.png"/>
                            </image>
                        </ImageView>
                        <AnchorPane id="AnchorPane" layoutX="86.0" layoutY="0.0">
                            <children>
                                <Label layoutY="35.0" styleClass="main-label-size" text="文件夹" textAlignment="CENTER"
                                       underline="false" wrapText="false">
                                    <font>
                                        <Font size="18.0"/>
                                    </font>
                                </Label>
                                <Label layoutX="0.0" layoutY="77.0" prefHeight="41.0" text="WebApp 标准的格式。"
                                       wrapText="true"/>
                            </children>
                        </AnchorPane>
                    </children>
                    <padding>
                        <Insets left="10.0" right="10.0"/>
                    </padding>
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                </AnchorPane>
            </children>
        </VBox>
        <VBox fx:id="containerOutputMBTiles" alignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="1">
            <children>
                <AnchorPane fx:id="panelOutputMBTiles" minHeight="-Infinity" prefHeight="-1.0" prefWidth="-1.0"
                            VBox.vgrow="NEVER">
                    <children>
                        <ImageView cache="false" fitHeight="58.0" fitWidth="86.0" focusTraversable="true" layoutX="0.0"
                                   layoutY="54.0" pickOnBounds="true" preserveRatio="false" rotate="0.0" smooth="false"
                                   visible="true">
                            <image>
                                <Image url="@../../../img/outputMbtiles.png"/>
                            </image>
                        </ImageView>
                        <AnchorPane id="AnchorPane" layoutX="86.0" layoutY="0.0">
                            <children>
                                <Label layoutY="35.0" styleClass="main-label-size" text="MBTiles" textAlignment="CENTER"
                                       underline="false" wrapText="false">
                                    <font>
                                        <Font size="18.0"/>
                                    </font>
                                </Label>
                                <Label layoutX="0.0" layoutY="77.0" prefHeight="41.0" text="用于移动设备，不支持矢量。"
                                       wrapText="true"/>
                            </children>
                        </AnchorPane>
                    </children>
                    <padding>
                        <Insets left="10.0" right="10.0"/>
                    </padding>
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                </AnchorPane>
            </children>
        </VBox>
        <VBox fx:id="containerOutputGeopackage" alignment="CENTER" prefHeight="321.0" prefWidth="247.0"
              GridPane.columnIndex="2" GridPane.rowIndex="1">
            <children>
                <AnchorPane fx:id="panelOutputGeopackage" minHeight="-Infinity" prefHeight="-1.0" prefWidth="-1.0"
                            VBox.vgrow="NEVER">
                    <children>
                        <ImageView cache="false" fitHeight="58.0" fitWidth="86.0" focusTraversable="true" layoutX="0.0"
                                   layoutY="54.0" pickOnBounds="true" preserveRatio="false" rotate="0.0" smooth="false"
                                   visible="true">
                            <image>
                                <Image url="@../../../img/outputGeopackage.png"/>
                            </image>
                        </ImageView>
                        <AnchorPane id="AnchorPane" layoutX="86.0" layoutY="0.0">
                            <children>
                                <Label layoutY="35.0" styleClass="main-label-size" text="GeoPackage"
                                       textAlignment="CENTER" underline="false" wrapText="false">
                                    <font>
                                        <Font size="18.0"/>
                                    </font>
                                </Label>
                                <Label layoutX="0.0" layoutY="77.0" prefHeight="41.0" text="球面墨卡托投影，用于GIS和移动应用"
                                       wrapText="true"/>
                            </children>
                        </AnchorPane>
                    </children>
                    <padding>
                        <Insets left="10.0" right="10.0"/>
                    </padding>
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                </AnchorPane>
            </children>
        </VBox>
        <VBox alignment="TOP_CENTER" GridPane.columnIndex="1" GridPane.rowIndex="2">
            <children>
                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label text="级别从 "/>
                        <ComboBox fx:id="cmbZoomFrom" prefHeight="23.0" prefWidth="60.0"/>
                        <Label text=" 到 "/>
                        <ComboBox fx:id="cmbZoomTo" prefHeight="23.0" prefWidth="60.0"/>
                    </children>
                </HBox>
                <Button fx:id="btnSettingAdvance" mnemonicParsing="false" text="高级设置">
                    <font>
                        <Font size="15.0"/>
                    </font>
                </Button>
            </children>
        </VBox>
        <VBox alignment="BOTTOM_CENTER" GridPane.columnIndex="1">
            <children>
                <Label text="切片设置" textAlignment="CENTER">
                    <font>
                        <Font size="30.0"/>
                    </font>
                </Label>
            </children>
        </VBox>
    </children>
    <opaqueInsets>
        <Insets bottom="50.0" left="80.0" right="80.0" top="50.0"/>
    </opaqueInsets>
</GridPane>
